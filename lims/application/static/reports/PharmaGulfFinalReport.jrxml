	<queryString language="SQL">
		<![CDATA[select 
acc.accession_id as case_num,
mdat.accession_type,
mr.methodology,
mdc.sponsor_name as name,
mdc.sponsor_address_info as address,
sub.subject_id as patient_name,
TO_CHAR(collection_dt::DATE, 'DD/MM/YYYY') collection_dt,
TO_CHAR(receive_dt::TIMESTAMP, 'DD/MM/YYYY HH24:MI') received_dt,
pat.mrn,
pat.gender,
TO_CHAR(pat.birth_dt::DATE, 'DD/MM/YYYY') dob,
concat(EXTRACT(YEAR FROM AGE(NOW(), pat.birth_dt::DATE)), ' y.o.') AS age,
concat(phy.first_name, ' ', phy.last_name) as physician_name,
TO_CHAR(NOW()::TIMESTAMP, 'DD/MM/YYYY HH24:MI') current_date,
STRING_AGG(distinct tcc.cpt_code, ', ') AS cpt_codes,
STRING_AGG(distinct tic.icd_code, ', ') AS icd_codes,
concat(su.first_name, ' ', su.last_name) as created_by,
bpa.investigator_id,
bpa.visit_id,
bp.project_protocol_id,
mr.amendment_type,
mr.amendment_comments
from analysis_mergereporting mr 
inner join accessioning_accession acc on mr.accession_id_id = acc.accession_id
inner join accessioning_biopharmaaccession bpa ON bpa.accession_ptr_id = acc.accession_id
inner join masterdata_patient pat on pat.patient_id = acc.patient_id_id
inner join masterdata_subject sub on sub.patient_ptr_id = pat.patient_id
left join masterdata_physician phy on acc.doctor_id = phy.physician_id
left  join accessioning_accessionicdcodemap aim on aim.accession_id_id = acc.accession_id
left join tests_icdcode tic on tic.icd_code_id = aim.icd_code_id_id
inner join analysis_mergereportingdtl mrd on mr.merge_reporting_id = mrd.merge_reporting_id_id
inner join analysis_reportoption ro on ro.report_option_id = mrd.report_option_id_id
inner join sample_sampletestmap stm on ro.root_sample_id_id = stm.sample_id_id
inner join tests_test tc on tc.test_id = stm.test_id_id
left join tests_testcptcodemap tcm on tcm.test_id_id = tc.test_id
left join tests_cptcode tcc on tcc.cpt_code_id = tcm.cpt_code_id_id
inner join security_user su on su.id = mr.assign_pathologist_id
inner join masterdata_accessiontype mdat on mdat.accession_type_id = acc.accession_type_id
inner join masterdata_sponsor mdc on mdc.sponsor_id = bpa.sponsor_id 
inner join masterdata_bioproject bp on bpa.project_id = bp.bioproject_id
where mr.merge_reporting_id =  $P{mergereportid} 
group by 
acc.accession_id,
mdat.accession_type,
mr.methodology,
mdc.sponsor_name,
mdc.sponsor_address_info,
sub.subject_id,
acc.collection_dt,
acc.receive_dt,
pat.mrn,
pat.gender,
pat.birth_dt,
phy.first_name,
phy.last_name,
su.first_name,
su.last_name,
bpa.investigator_id,
bpa.visit_id,
bp.project_protocol_id,
mr.amendment_type,
mr.amendment_comments]]>
